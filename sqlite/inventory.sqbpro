CREATE TABLE inventory (
id_item INTEGER PRIMARY KEY NOT NULL,
reference_group_id INTEGER NOT NULL,
reference_country_id INTEGER NOT NULL,
description TEXT,
brand TEXT,
model TEXT,
purchase_date DATE,
