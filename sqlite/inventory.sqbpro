reference_country_id INTEGER NOT NULL,

FOREIGN KEY (reference_type_id) REFERENCES inventory_type(id_type),
FOREIGN KEY (reference_country_id) REFERENCES inventory_country(id_country)
);


CREATE TABLE inventory_condition (
